  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 20
    },
    "format": "{icon} {capacity}%",
    "format-charging": "Ôá¶ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-alt": "{time} {icon}",
    "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"]
  },

  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-muted": "ÔÄ¶ Muted",
    "on-click": "~/.config/waybar/scripts/volume --toggle",
    "on-click-right": "pavucontrol -t 3",
    "on-scroll-up": "~/.config/waybar/scripts/volume --inc",
    "on-scroll-down": "~/.config/waybar/scripts/volume --dec",
    "scroll-step": 5,
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
    },
    "tooltip": true,
    "tooltip-format": "{icon} at {volume}%"
  },

  "pulseaudio#microphone": {
    "format": "{format_source}",
    "format-source": "ÔÑ∞ {volume}%",
    "format-source-muted": "ÔÑ∞ Muted",
    "on-click": "~/.config/waybar/scripts/volume --toggle-mic",
    "on-scroll-up": "~/.config/waybar/scripts/volume --mic-inc",
    "on-scroll-down": "~/.config/waybar/scripts/volume --mic-dec",
    "scroll-step": 5,
    "tooltip": false,
//    "tooltip-format": "ÔÑ∞ at {volume}%"
  },

  "mpris": {
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "default": "‚è∏",
      "mpv": "üéµ"
    },
    "status-icons": {
      "paused": "‚ñ∂"
    },
    "interval": 1,
    "max-length": 60
  },

  "backlight": {
    "device": "radeon_bl0",
    "format": "{icon} {percent}%",
    "tooltip": false,
    "format-icons": ["Û∞Éû", "Û∞Éü", "Û∞É†"],
    "on-scroll-up": "~/.config/waybar/scripts/brightness --inc",
    "on-scroll-down": "~/.config/waybar/scripts/brightness --dec",
    "min-length": 6,
    "on-click": "killall wlsunset",
    "on-click-right": "wlsunset -t 4500 -T 5500"
  },

 "custom/notification": {
    "tooltip": false,
    "format": "{icon}",
    "format-icons": {
      "notification": "Û∞Çö<span foreground='#F7768E'><sup>ÔëÑ</sup></span>",
      "none": "Û∞Çö",
      "dnd-notification": "Û∞Çõ<span foreground='#F7768E'><sup>ÔëÑ</sup></span>",
      "dnd-none": "Û∞Çõ",
      "inhibited-notification": "Û∞Çö<span foreground='#F7768E'><sup>ÔëÑ</sup></span>",
      "inhibited-none": "Û∞Çö",
      "dnd-inhibited-notification": "Û∞Çõ<span foreground='#F7768E'><sup>ÔëÑ</sup></span>",
      "dnd-inhibited-none": "Û∞Çõ"
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "sleep 0.1 && swaync-client -swb",
    "on-click": "sleep 0.1 && swaync-client -t -sw",
    "on-click-right": "sleep 0.1 && swaync-client -d -sw",
    "escape": true
  },

 "custom/power": {
   "format": " ÔÄë ",
   "tooltip": false,
   "on-click": "killall rofi || ~/.config/rofi/powermenu/type-5/powermenu.sh"
  }

