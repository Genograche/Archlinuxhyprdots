{
  "layer": "bottom",
  "position": "top",
  "mod": "dock",
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  "height": 0,
  "modules-left": [
    "clock",
    "idle_inhibitor",
    "cpu",
    "memory",
    "disk",
    "temperature",
//  "custom/weather",
    "wlr/taskbar"
  ],
  "modules-center": ["hyprland/workspaces"],
  "modules-right": [
    "mpris",
//  "bluetooth",
    "network",
    "tray",
    "pulseaudio",
    "pulseaudio#microphone",
    "battery",
    "custom/notification",
    "backlight",
    "custom/power"
  ],
 
  "hyprland/workspaces": {
    "disable-scroll": false,
    "all-outputs": true,
    "on-click": "activate",
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "format": "{icon}",
    "format-icons": {
//      "1": "‚ûä",
//      "2": "‚ûã",
      "active": "Û∞ÆØ",
      "default":"Û∞ä†",
//      "default":"Û∞ßµ",
      "empty": "Û∞ä†",
//      "special":"Ôä¨",
      "urgent": "ÔÅ™"
  },

  "persistent-workspaces": {
      "1": [],
      "2": [],
      "3": [],
      "4": [],
      "5": []
    }
  },

  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 18,
    "icon-theme": "Papirus",
    "spacing": 0,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": [
      "Alacritty",
      "Rofi"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition",
      "ru-turikhay-tlauncher-bootstrap-Bootstrap":"lutris"
    }
  },

  "tray": {
    "icon-size": 18,
    "spacing": 5
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "start-activated": true,
    "format-icons": {
      "activated": "ÔàÖ",
      "deactivated": "ÔàÑ"
      }
  },

  "clock": {
        "format": "{:%I:%M %p}",
        "format-alt": "{:%R Û∞É≠ %d¬∑%m¬∑%y}",
        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "mode": "month",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#ffead3'><b>{}</b></span>",
                "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
                "today": "<span color='#ff6699'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
  },

  "network": {
        "tooltip": true,
//        "format-wifi": "Ôá´ ",
//        "format-ethernet": "Û∞àÄ ",
        "format-wifi": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        "format-ethernet": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        "tooltip-format": "Network: <big><b>{essid}</b></big>\nSignal strength: <b>{signaldBm}dBm ({signalStrength}%)</b>\nFrequency: <b>{frequency}MHz</b>\nInterface: <b>{ifname}</b>\nIP: <b>{ipaddr}/{cidr}</b>\nGateway: <b>{gwaddr}</b>\nNetmask: <b>{netmask}</b>",
        "format-linked": "Û∞àÄ {ifname} (No IP)",
        "format-disconnected": " Û∞ñ™ ",
        "tooltip-format-disconnected": "Disconnected",
//        "format-alt": "{essid}",
//        "format-alt": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        "interval": 2,
        "on-click": "~/.config/waybar/scripts/network.py",
  },
//  "bluetooth": {
//    "exec": "~/.config/waybar/scripts/rofi-bluetooth-menu --status",
//    "format-disabled": "", // an empty format will hide the module
//    "format-connected": "Ôäì {num_connections}",
//    "tooltip-format": "{device_alias}",
//    "tooltip-format-connected": "Ôäì {device_enumerate}",
//    "tooltip-format-enumerate-connected": "{device_alias}",
//    "on-click-right": "~/.config/waybar/scripts/rofi-bluetooth-menu"
//  },

  "memory": {
    "interval": 1,
    "format": "Ôãõ {}%",
    "tooltip": true,
    "tooltip-format": "Memory - {used:0.1f}GB used",
    "on-click": "gnome-system-monitor",
    "on-click-right": "alacritty -e htop"
  },
  "cpu": {
    "interval": 1,
    "format": "Ôíº {usage}%",
    "on-click": "gnome-system-monitor"
  },

  "disk": {
    "interval": 1,
    "format": "Û∞ãä {percentage_used}%",
    "path": "/",
    "format-alt-click": "click-right",
    "format-alt": "Û∞ãä {percentage_used}%",
    "tooltip": true,
    "tooltip-format": "Disk - {used} used out of {total} on {path} ({percentage_used}%)",
    "on-click": "gnome-system-monitor"
  },

  "temperature": {
    "interval": 1,
    "thermal-zone": 0,
    "format": "Ôãâ {temperatureC}¬∞C",
    "critical-threshold": 80,
    "format-critical": "Ôãá {temperatureC}¬∞C",
    "on-click": "gnome-system-monitor"
  },
